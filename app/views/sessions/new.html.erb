<h1>Log In Here:</h1>

<p><strong><%= flash[:error] %></strong></p>
<hr>

<%= form_for :user do |f| %>

    <%= f.label :email %>:&nbsp;&nbsp;&emsp;
    <%= f.email_field :email %><br><br>

    <%= f.label :password %>:
    <%= f.password_field :password %><br><br>

    <%= f.submit "Log In"%>
<% end %>